{
  "name": "PandaScope",
  "description": "Check your HTTP requests",
  "version": "0.1.0",
  "manifest_version": 3,
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  },
  "background": {
      "service_worker": "./background.js"
  },
  "action": {
      "default_popup": "./window.html",
      "default_icons": {
        "16": "icon_16.png",
        "128": "icon_128.png"
      }
  },
  "permissions": [
      "storage",
      "webRequest"
  ],
  "host_permissions": [
      "<all_urls>"
  ]
}